# OpenAI API key for LLM usage
OPENAI_API_KEY=your_openai_api_key_here

# Qdrant credentials for vector database
QDRANT_API_KEY=your_qdrant_api_key_here
QDRANT_URL=your_qdrant_url_here

# Qdrant collection details - configure as needed
QDRANT_COLLECTION=dev-collection
QDRANT_VECTOR_SIZE=3072
TEXT_PAYLOAD_KEY=content
# Gemini API key (Google Generative AI)
GEMINI_API_KEY=your_gemini_api_key_here

# Cartesia API key (voice cloning, TTS, etc.)
CARTESIA_API_KEY=your_cartesia_api_key_here

QDRANT_COLLECTION=your_collection_name

# LiveKit Configuration (if needed)
LIVEKIT_URL=wss://your-livekit-instance.livekit.cloud
LIVEKIT_API_KEY=your_livekit_api_key
LIVEKIT_API_SECRET=your_livekit_api_secret